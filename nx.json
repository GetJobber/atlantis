{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "master",
  "targetDefaults": {
    "bootstrap": {
      "cache": true,
      "inputs": [
        "default",
        "^default"
      ],
      "outputs": ["{projectRoot}/dist"],
      "dependsOn": ["^bootstrap"]
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "!{projectRoot}/node_modules/**/*",
      "!{projectRoot}/dist/**/*"
    ]
  }
}
