{
  "defaultSeverity": "error",
  "extends": [
    "jobber-tslint-foundation",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "import-name": [true, {
      "react": "React",
      "*.css": "styles"
    }],
    "missing-jsdoc": false,
    "quotemark": [true, "single", "avoid-escpate", "avoid-template", "jsx-double"],
    "prefer-template": true,
    "no-default-export": false,
    "no-relative-imports": [true, "allow-siblings"]
  },
  "rulesDirectory": []
}
