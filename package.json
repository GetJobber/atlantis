{
  "private": true,
  "name": "atlantis",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run buildHooksIndex",
    "start": "npm run storybook",
    "start:withPrivateComponents": "USE_ATLANTIS_ALIASES=true PRIVATE_COMPONENTS=visible docz dev",
    "test": "jest",
    "plop": "echo 'ðŸš§ `npm run plop` is now `npm run generate`'",
    "generate": "plop -- --path='./packages/components/src/'",
    "build": "npm run build-storybook -- -o .docz/dist",
    "clean": "rm -rf .docz",
    "lerna:bootstrap": "./scripts/bootstrap.sh",
    "lerna:compile-typescript": "lerna run ci:typescript-compile",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:css": "stylelint 'packages/**/*.css' --allow-empty-input",
    "lint:markdown": "prettier --check '**/*.{md,mdx}' --write",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:css -- --fix && npm run lint:markdown -- --write",
    "install": "npm run lerna:bootstrap",
    "release-the-kraken": "export IGNORE_OPTIONAL_DEPENDENCIES=true; npm ci --no-optional && lerna publish",
    "release:unpublished-package": "export IGNORE_OPTIONAL_DEPENDENCIES=true; npm ci --no-optional && lerna publish from-package",
    "publish:prerelease": "lerna publish --canary --dist-tag next --preid pre from-package",
    "ci:release": "lerna publish --yes",
    "version": "node ensureChangelogHeaders.js",
    "convertIcoMoon": "scripts/convertIcoMoon.ts",
    "preventManualRelease": "node scripts/preventManualRelease.js",
    "buildHooksIndex": "node scripts/createIndexFile.js ./packages/hooks/src",
    "lerna:clean": "lerna run clean && lerna clean --y && npm run clean:packages && npm run clean",
    "clean:packages": "rm -rf node_modules **/dist",
    "storybook": "start-storybook -p 6005",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GetJobber/atlantis.git"
  },
  "author": "The Frends",
  "bugs": {
    "url": "https://github.com/GetJobber/atlantis/issues"
  },
  "homepage": "https://github.com/GetJobber/atlantis#readme",
  "devDependencies": {
    "@apollo/client": "^3.3.16",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@jobber/components": "file:packages/components",
    "@jobber/design": "file:packages/design",
    "@jobber/docx": "file:packages/docx",
    "@jobber/docz-tools": "file:packages/docz-tools",
    "@jobber/eslint-config": "file:packages/eslint-config",
    "@jobber/generators": "file:packages/generators",
    "@jobber/stylelint-config": "file:packages/stylelint-config",
    "@reach/router": "^1.3.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@storybook/theming": "^6.5.16",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^12.0.2",
    "@types/history": "^4.7.6",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/theme-ui": "^0.3.7",
    "autoprefixer": "^9.8.4",
    "axios": "^0.21.4",
    "babel-loader": "^8.3.0",
    "chalk": "^4.1.2",
    "css-loader": "^3.4.2",
    "docz": "^2.3.1",
    "eslint": "^6.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-mdx": "^1.16.0",
    "gatsby-theme-docz": "^2.3.1",
    "glob": "^7.1.4",
    "husky": "^2.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-junit": "^6.4.0",
    "lerna": "^3.20.2",
    "lint-staged": "^9.4.2",
    "lodash": "^4.17.21",
    "plop": "^2.5.3",
    "postcss": "^7.0.36",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.4",
    "react-router-dom": "~5.2.0",
    "storybook-addon-designs": "^6.3.1",
    "style-loader": "^0.23.1",
    "stylelint": "^14.2.0",
    "stylelint-junit-formatter": "^0.2.1",
    "typed-css-modules": "^0.7.0",
    "typed-css-modules-loader": "0.0.18",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "iOS 9",
    "ie >= 11"
  ],
  "dependencies": {
    "@std-proposal/temporal": "0.0.1"
  },
  "optionalDependencies": {
    "@jobber/fonts": "^1.1.0"
  }
}
