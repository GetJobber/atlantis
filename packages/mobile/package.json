{
  "name": "mobile",
  "version": "1.0.0",
  "main": "dis/cjs/index.js",
  "scripts": {
    "build": "rm -rf /dist && npm run build:esm && npm run build:cjs",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
