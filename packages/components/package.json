    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "copyfiles": "^2.1.0",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.11.3",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.21.0",
    "typescript": "^3.4.5"
  }
