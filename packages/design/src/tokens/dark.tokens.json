{
    "color": {
        "$type": "color",
        "$description:": "Color tokens for the dark theme.",
        "base": {
            "blue-": {
                "700": {
                    "$value": "hsl(197, 15%, 35%)"
                },
                "800": {
                    "$value": "hsl(197, 18%, 25%)"
                },
                "900": {
                    "$value": "hsl(197, 15%, 16%)"
                },
                "1000": {
                    "$value": "hsl(197, 10%, 10%)"
                }
            }
        },
        "interactive": {
            "$value": "hsl(86, 60%, 50%)",
            "$description": "The default color used for interactive elements."
        },
        "interactive-": {
            "hover": {
                "$value": "hsl(86, 60%, 60%)"
            },
            "subtle": {
                "$value": "{color.base.blue-.200}",
                "$description": "Use for actions that do not demand prominence, such as closing a modal or a navigation icon that is present on every view."
            },
            "subtle-": {
                "hover": {
                    "$value": "{color.base.blue-.100}"
                }
            }
        },
        "destructive": {
            "$value": "{color.base.red-.500}",
            "$description": "Use to signify that an interaction will destroy something in the users’ account or workflow."
        },
        "destructive-": {
            "hover": {
                "$value": "{color.base.red-.400}"
            }
        },
        "disabled": {
            "$value": "{color.base.grey-.500}",
            "$description": "Use to signify that an interactive element is disabled."
        },
        "disabled-": {
            "secondary": {
                "$value": "{color.base.grey-.700}",
                "$description": "Use when a disabled element needs more than one shade."
            }
        },
        "critical": {
            "$value": "{color.base.red-.500}",
            "$description": "Action required; user must see this status to be unblocked."
        },
        "critical-": {
            "surface": {
                "$value": "{color.base.red-.900}"
            },
            "onSurface": {
                "$value": "{color.base.red-.300}"
            }
        },
        "warning": {
            "$value": "{color.base.yellow-.400}",
            "$description": "Action may be required as a consequence of current state."
        },
        "warning-": {
            "surface": {
                "$value": "{color.base.yellow-.700}"
            },
            "onSurface": {
                "$value": "{color.base.yellow-.200}"
            }
        },
        "success": {
            "$value": "{color.base.green-.500}",
            "$description": "No action required; an action has completed successfully."
        },
        "success-": {
            "surface": {
                "$value": "{color.base.green-.800}"
            },
            "onSurface": {
                "$value": "{color.base.green-.300}"
            }
        },
        "informative": {
            "$value": "{color.base.lightBlue-.600}",
            "$description": "No action required; but helpful to know about."
        },
        "informative-": {
            "surface": {
                "$value": "{color.base.lightBlue-.800}"
            },
            "onSurface": {
                "$value": "{color.base.lightBlue-.300}"
            }
        },
        "inactive": {
            "$value": "{color.base.blue-.500}",
            "$description": "No action required; not part of an active workflow."
        },
        "inactive-": {
            "surface": {
                "$value": "{color.base.blue-.700}"
            },
            "onSurface": {
                "$value": "{color.base.blue-.200}"
            }
        },
        "heading": {
            "$value": "{color.base.blue-.100}",
            "$description": "Headings have a bold, dark color to cement their hierarchy."
        },
        "text": {
            "$value": "{color.base.blue-.200}",
            "$description": "A slightly softer color is used for body text for greater readability."
        },
        "text-": {
            "secondary": {
                "$value": "{color.base.blue-.400}",
                "$description": "Text that is relevant but less important can be lower-contrast to suggest its’ reduced importance."
            },
            "reverse": {
                "$value": "{color.base.blue-.700}",
                "$description": "Use on “reversed” (light) sections."
            },
            "reverse-": {
                "secondary": {
                    "$value": "{color.base.blue-.900}",
                    "$description": "Secondary text for “reversed” sections."
                }
            }
        },
        "icon": {
            "$value": "{color.base.blue-.200}"
        },
        "surface": {
            "$value": "{color.base.blue-.900}",
            "$description": "Most elements in Jobber have a white surface; if clickable, they have a light hover and active color."
        },
        "surface-": {
            "hover": {
                "$value": "{color.base.blue-.800}"
            },
            "active": {
                "$value": "{color.base.blue-.700}"
            },
            "background": {
                "$value": "{color.base.blue-.1000}",
                "$description": "A slightly darker surface gives a receded appearance relative to main surfaces."
            },
            "background-": {
                "hover": {
                    "$value": "{color.base.blue-.800}"
                },
                "subtle": {
                    "$value": "{color.base.blue-.800}",
                    "$description": "Use when a subtler distinction than the standard surface--background is needed."
                }
            },
            "reverse": {
                "$value": "{color.base.taupe-.200}",
                "$description": "When a strong constrast is needed with the rest of the application, a reversed surface can be used."
            }
        },
        "border": {
            "$value": "{color.base.blue-.700}",
            "$description": "Most of our borders should use this color for subtle definition."
        },
        "border-": {
            "interactive": {
                "$value": "{color.base.blue-.500}",
                "$description": "Use for the borders of interactive elements to give greater visual significance and ensure the element meets WCAG contrast standards."
            },
            "section": {
                "$value": "{color.base.blue-.400}",
                "$description": "Use where other bordered content is being further sectioned, such as table headers or list sections."
            }
        },
        "overlay": {
            "$value": "rgba({color.black-.rgb}, 0.6)",
            "$description": "Use to mask an area of the interface to promote focus to a foreground action, such as a Modal’s appearance."
        },
        "overlay-": {
            "dimmed": {
                "$value": "rgba({color.black-.rgb}, 0.4)",
                "$description": "A dimmed version of Surface, this masks an area of the interface to indicate inactivity (i.e. waiting for updates)."
            }
        },
        "request": {
            "$value": "{color.base.orange-.500}",
            "$description": "Requests are orange."
        },
        "quote": {
            "$value": "{color.base.pink-.500}",
            "$description": "Quotes are pink."
        },
        "job": {
            "$value": "{color.base.green-.500}",
            "$description": "Jobs are green because of their core association to Jobber, which has historically used some shade of green for branding."
        },
        "visit": {
            "$value": "{color.job}",
            "$description": "Visits inherit the green from jobs."
        },
        "task": {
            "$value": "{color.base.blue-.500}",
            "$description": "Tasks use a deeper shade of blue than invoices and the two rarely share interfaces as tasks are generally isolated to scheduling."
        },
        "event": {
            "$value": "{color.base.yellow-.400}",
            "$description": "Events use yellow."
        },
        "invoice": {
            "$value": "{color.base.lightBlue-.500}",
            "$description": "Invoices are blue as this is a common color associated with banking and finance."
        },
        "payments": {
            "$value": "{color.invoice}",
            "$description": "Payments inherit the blue of invoicing."
        },
        "request-": {
            "surface": {
                "$value": "{color.base.orange-.800}"
            },
            "onSurface": {
                "$value": "{color.base.orange-.300}"
            }
        },
        "quote-": {
            "surface": {
                "$value": "{color.base.pink-.800}"
            },
            "onSurface": {
                "$value": "{color.base.pink-.300}"
            }
        },
        "job-": {
            "surface": {
                "$value": "{color.base.green-.800}"
            },
            "onSurface": {
                "$value": "{color.base.green-.200}"
            }
        },
        "visit-": {
            "surface": {
                "$value": "{color.job-.surface}"
            },
            "onSurface": {
                "$value": "{color.job-.onSurface}"
            }
        },
        "task-": {
            "surface": {
                "$value": "{color.base.blue-.700}"
            },
            "onSurface": {
                "$value": "{color.base.blue-.300}"
            }
        },
        "event-": {
            "surface": {
                "$value": "{color.base.yellow-.700}"
            },
            "onSurface": {
                "$value": "{color.base.yellow-.200}"
            }
        },
        "invoice-": {
            "surface": {
                "$value": "{color.base.lightBlue-.800}"
            },
            "onSurface": {
                "$value": "{color.base.lightBlue-.300}"
            }
        },
        "payments-": {
            "surface": {
                "$value": "{color.invoice-.surface}"
            },
            "onSurface": {
                "$value": "{color.invoice-.surface}"
            }
        },
        "client": {
            "$value": "{color.base.blue-.200}",
            "$description": "Clients use a neutral value as they sit at the core of the workflow."
        }
    },
    "shadow": {
        "$type": "shadow",
        "$description": "Shadow tokens for the dark theme.",
        "low": {
            "$value": "0px var(--space-minuscule) var(--space-smallest) rgba(0, 0, 0, 0.15), 0px 0px var(--space-smallest) rgba(0, 0, 0, 0.25)"
        },
        "base": {
            "$value": "0px var(--space-minuscule) var(--space-smaller) 0px rgba(0, 0, 0, 0.1), 0px var(--space-smaller) 12px 0px rgba(0, 0, 0, 0.2)"
        },
        "high": {
            "$value": "0px var(--space-base) var(--space-base) 0px rgba(0, 0, 0, 0.075), 0px 0px var(--space-small) 0px rgba(0, 0, 0, 0.175)"
        }
    },
    "field-": {
        "placeholder": {
            "color": {
                "$value": "{color.base.blue-.400}"
            }
        },
        "border": {
            "color": {
                "$value": "{color.base.blue-.500}"
            }
        }
    }
}
