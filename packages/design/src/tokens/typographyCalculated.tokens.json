{
  "typography": {
    "_": {
      "letterSpacing": {
        "loose": {
          "value": "calc({base.unit} / 40)"
        }
      },
      "fontSize": {
        "extravagant": {
          "value": "calc({typography._.fontSize.large} * 3)"
        },
        "jumbo": {
          "value": "calc({typography._.fontSize.large} * 2.25)"
        },
        "largest": {
          "value": "calc({typography._.fontSize.large} * 1.5)"
        },
        "larger": {
          "value": "calc({typography._.fontSize.large} * 1.25)"
        },
        "large": {
          "value": "calc({base.unit} * 1)"
        },
        "base": {
          "value": "calc({typography._.fontSize.large} * 0.875)"
        },
        "small": {
          "value": "calc({typography._.fontSize.large} * 0.75)"
        },
        "smaller": {
          "value": "calc({typography._.fontSize.large} * 0.625)"
        }
      }
    }
  }
}