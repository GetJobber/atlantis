{
  "name": "@jobber/hooks",
  "version": "2.11.1",
  "license": "MIT",
  "main": "./dist/index.mjs",
  "type": "module",
  "types": "./dist/index.d.mts",
  "scripts": {
    "build": "tsc --project . --skipLibCheck && (mv dist/index.js dist/index.mjs || true) 2> /dev/null && (mv dist/index.d.ts dist/index.d.mts || true) 2> /dev/null",
    "clean": "rm -rf dist/* tsconfig.tsbuildinfo",
    "bootstrap": "npm run clean; npm run build",
    "entryPoints:build": "ts-node --project ../../tsconfig.bin.json ../../scripts/entryPoints.ts",
    "entryPoints:clean": "npm run entryPoints:build clean",
    "prepack": "npm run entryPoints:build",
    "postpack": "npm run entryPoints:clean"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "dist/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./dist": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useBreakpoints": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useCollectionQuery": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useOnMount": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useFormState": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useBool": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useIsMounted": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useWindowDimensions": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useOnKeyDown": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    },
    "./useResizeObserver": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@jobber/formatters": "^0.3.0",
    "@types/lodash": "4.14.136",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^8.3.3",
    "@types/zxcvbn": "^4.4.1",
    "jsdom-testing-mocks": "^1.9.0",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "resize-observer-polyfill": "^1.5.1",
    "ts-xor": "^1.0.8",
    "use-resize-observer": "^6.1.0",
    "zxcvbn": "^4.4.2"
  },
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "react": "^18.2.0"
  }
}