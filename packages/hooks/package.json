    "./useSafeLayoutEffect": {
      "types": "./dist/useSafeLayoutEffect/index.d.ts",
      "import": "./dist/useSafeLayoutEffect/useSafeLayoutEffect.js",
      "require": "./dist/useSafeLayoutEffect/index.cjs"
    },
    "./useShowClear": {
      "types": "./dist/useShowClear/index.d.ts",
      "import": "./dist/useShowClear/useShowClear.js",
      "require": "./dist/useShowClear/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup --format cjs,esm",
    "postbuild":"tsc --skipLibCheck --project .",
    "clean": "rm -rf dist/* tsconfig.tsbuildinfo",
    "bootstrap": "npm run clean; npm run build",
    "entryPoints:build": "ts-node --project ../../tsconfig.bin.json ../../scripts/entryPoints.ts",
    "entryPoints:clean": "npm run entryPoints:build clean",
    "prepack": "npm run entryPoints:build",
    "postpack": "npm run entryPoints:clean"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "dist/*"
  ],
  "devDependencies": {
    "@apollo/react-testing": "^4.0.0",
    "@jobber/formatters": "*",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/lodash": "4.14.136",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^8.3.3",
    "@types/zxcvbn": "^4.4.1",
    "jsdom-testing-mocks": "^1.9.0",
    "tsup": "^8.0.2",
    "typescript": "^4.9.5",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "lodash": "^4.17.20",
