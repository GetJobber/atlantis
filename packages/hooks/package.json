      "require": "./dist/useLiveAnnounce/index.cjs"
    },
    "./useOnKeyDown": {
      "types": "./dist/useOnKeyDown/index.d.ts",
      "import": "./dist/useOnKeyDown/useOnKeyDown.js",
      "require": "./dist/useOnKeyDown/index.cjs"
    },
    "./usePasswordStrength": {
      "types": "./dist/usePasswordStrength/index.d.ts",
      "import": "./dist/usePasswordStrength/usePasswordStrength.js",
      "require": "./dist/usePasswordStrength/index.cjs"
    },
    "./useRefocusOnActivator": {
