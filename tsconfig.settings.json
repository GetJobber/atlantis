{
  "compilerOptions": {
    "pretty": true,
    "noEmitOnError": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,

    "allowSyntheticDefaultImports": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["es6", "dom"],
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react"
  }
}
